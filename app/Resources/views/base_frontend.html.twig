        {% block javascripts %}

            <script type="text/javascript" src="{{asset('front/plugins/jquery/jquery.min.js')}}"></script>
            <script type="text/javascript" src="{{asset('front/plugins/bootstrap/js/bootstrap.min.js')}}"></script>
            <script type="text/javascript" src="{{asset('front/plugins/Sequence/scripts/jquery.sequence-min.js')}}"></script>    

            <!-- JS Global Compulsory -->			
            <script>
		(function($){
			/// slider
			function slideh(){
				var ancho = $('#sequence').width();
				var alto = ancho / 3;
				$('#sequence').height(alto);	
			};
			
			slideh();
			
			$(window).resize(function() {
				slideh();
			});
			
			var options = {
				//pagination: true,
				animateStartingFrameIn: true,
				autoPlay: true,
				autoPlayDelay: 3000,
				pauseOnHover: false
			};
			var mySequence = $("#sequence").sequence(options).data("sequence");
			
		}(jQuery));
		</script>

